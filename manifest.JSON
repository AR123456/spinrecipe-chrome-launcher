    "manifest_version":2,
    "name":"Recipe Launcher",
    "description": "Quick launch recipe finder",
    "verson": "1.0.0",
    "icons":{"128": "./images/icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup":"popup.html"
    },
    "permissions":["activeTab"]
}